<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	    xmlns:context="http://www.springframework.org/schema/context"
	    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<bean id="emp" class="Lab1point2.Employee" autowire="byType">
		<property name="eid" value="101" />
		<property name="eName" value="Donald Trumph" />
		<property name="salary" value="15000.00" />
		<property name="BU" value="PES-Bu" />
		<property name="age" value="30" />
    </bean>
	 <bean id="sbu" class="Lab1point2.SBU">
		    <constructor-arg value="105" />
			<constructor-arg value="PSE-BU" />
			<constructor-arg value="Kiran Rao" />
	 </bean>
	
</beans>